{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/styles/*": ["src/styles/*"],
      "@/components/*": ["src/components/*"],
      "@/assets/*": ["src/assets/*"],
      "@/images/*": ["/src/assets/images/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
